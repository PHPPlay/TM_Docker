#!/bin/bash

echo -e '\n****** Starting TM and Cloud9 ******* \n'

cd /cloud9                                                              # starting cloud9
node server.js -p 8181 -l 0.0.0.0 -a : -w /root/workspace/TM  &         # on the folder we cloned TM

cd /root/workspace/TM                                                   # start TM servers
./bin/start-servers.sh
